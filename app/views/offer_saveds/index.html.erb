<p id="notice"><%= notice %></p>

<h1>Listing Offer Saveds</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Offer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offer_saveds.each do |offer_saved| %>
      <tr>
        <td><%= offer_saved.user %></td>
        <td><%= offer_saved.offer %></td>
        <td><%= link_to 'Show', offer_saved %></td>
        <td><%= link_to 'Edit', edit_offer_saved_path(offer_saved) %></td>
        <td><%= link_to 'Destroy', offer_saved, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offer saved', new_offer_saved_path %>
