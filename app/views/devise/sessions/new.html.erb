  <div class="container white-text">
    <h5 class="center">Iniciar Sesi칩n</h5>
<% if session_path(resource) == "/users/sign_in" %>
    <h5 class="center yellow-text">Administrador</h5>
  <% end %>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <%= devise_error_messages! %>

      <div class="field">
        <%= f.label 'Correo' %><br />
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="field">
        <%= f.label "Contrase침a" %><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

      <% if devise_mapping.rememberable? %>
        <p>
          <%= f.check_box :remember_me, id:"check_box1"%>
          <%= f.label "Recordar Contrase침a", for:"check_box1" %>
        </p>
      <% end %>


      <div style="margin-top:5%" class="actions">
        <button class="btn-large teal darken-2">
            Iniciar Sesi칩n
          <%f.submit "Log in" %>
        </button>
      </div>
    <% end %>

    <%= render "devise/shared/links" %>
  </div>
