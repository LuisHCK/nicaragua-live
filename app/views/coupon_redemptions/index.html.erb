<p id="notice"><%= notice %></p>

<h1>Coupon Redemptions</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coupon_redemptions.each do |coupon_redemption| %>
      <tr>
        <td><%= coupon_redemption.user %></td>
        <td><%= coupon_redemption.profile %></td>
        <td><%= link_to 'Show', coupon_redemption %></td>
        <td><%= link_to 'Edit', edit_coupon_redemption_path(coupon_redemption) %></td>
        <td><%= link_to 'Destroy', coupon_redemption, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coupon Redemption', new_coupon_redemption_path %>
